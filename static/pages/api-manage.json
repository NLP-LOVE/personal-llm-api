                    "copyable": true
                },
                {
                    "name": "input_unit_price",
                    "label": "模型输入单价/千token"
                },
                {
                    "name": "output_unit_price",
                    "label": "模型输出单价/千token"
                },
                {
                    "name": "create_time",
                    "label": "创建时间"
                },
                {
                    "type": "operation",
                    "label": "操作",
                    "buttons": [
                        {
                            "label": "修改",
                            "type": "button",
                            "actionType": "dialog",
                            "className": "m-b-sm",
                            "dialog": {
                                "title": "修改标签",
                                "body": {
                                    "type": "form",
                                    "api": "post:/api-manage/model/update",
                                    "body": [
                                        {
                                            "type": "input-text",
                                            "name": "id",
                                            "label": "ID",
                                            "disabled": true
                                        },
                                        {
                                            "name": "status",
                                            "type": "switch",
                                            "label": "状态"
                                        },
                                        {
                                            "type": "input-text",
                                            "name": "provider_english_name",
                                            "label": "供应商英文名称"

                                        },
                                        {
                                            "type": "input-text",
                                            "name": "model_name",
                                            "label": "model_name"
                                        },
                                        {
                                            "type": "input-text",
                                            "name": "model_version_name",
                                            "label": "model_version_name"
                                        },
                                        {
                                            "type": "input-text",
                                            "name": "model_short_name",
                                            "label": "model_short_name"
                                        },
                                        {
                                            "type": "input-text",
                                            "name": "input_unit_price",
                                            "label": "模型输入单价/千token"
                                        },
                                        {
                                            "type": "input-text",
                                            "name": "output_unit_price",
                                            "label": "模型输出单价/千token"
                                        }
                                    ]
                                }
                            }

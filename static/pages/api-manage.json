{
    "type": "page",
    "body": [
        {
            "label": "新增模型供应商",
            "type": "button",
            "actionType": "dialog",
            "level": "primary",
            "className": "m-b-sm",
            "dialog": {
                "title": "新增模型供应商",
                "body": {
                    "type": "form",
                    "api": "post:/api-manage/provider/create",
                    "body": [
                        {
                            "type": "input-text",
                            "name": "provider_name",
                            "label": "供应商名称"
                        },
                        {
                            "type": "input-text",
                            "name": "provider_english_name",
                            "label": "供应商英文名称"
                        },
                        {
                            "type": "input-text",
                            "name": "service_key",
                            "label": "api-key"
                        },
                        {
                            "type": "input-text",
                            "name": "service_url",
                            "label": "服务url"
                        }
                    ]
                }
            }
        },
        {
            "type": "crud",
            "api": "/api-manage/provider/list",
            "syncLocation": false,
            "columns": [
                {
                    "name": "row_id",
                    "label": "ID"
                },
                {
                    "name": "provider_name",
                    "label": "供应商名称"
                },
                {
                    "name": "provider_english_name",
